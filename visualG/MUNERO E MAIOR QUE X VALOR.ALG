Algoritmo "munero e maior que x valor"

Var
   n1, n2:inteiro

Inicio
   escreva("Digite 1ª número: ")
     leia(n1)
   escreva("Digite 2ª número: ")
     leia(n2)
   se (n1 > n2) entao
          escreva(n1, " é maior que",n2)

            senao
             escreva(n2, " é maior que",n1)
   fimse

Fimalgoritmo